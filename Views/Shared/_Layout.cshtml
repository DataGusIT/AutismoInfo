<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AutismoInfo</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha384-KyZXEAg3QhqLMpG8r+Knujsl5/1hb8Wl6r4dXp8r7uO3u/O5g2v6Y8b7Ckm5C9w5" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true">/>
    <style>
       
    </style>
</head>
<body>
    <nav class="navbar">
        <div class="container">
            <div class="nav-content">
                <div class="logo">
                    <i class="fas fa-infinity"></i>
                    <span class="brand-text">AutismoInfo</span>
                </div>
                <ul class="nav-links">
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Página Inicial</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Recursos</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Features" asp-action="Index">FAQ</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Testimonials" asp-action="Index">Contatos</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="FAQ" asp-action="Index">FAQ</a></li>
                </ul>
                <div>
                    <a class="btn btn-outline" asp-area="" asp-controller="Login" asp-action="Index">Entrar</a>
                    <a class="btn btn-primary" asp-area="" asp-controller="Account" asp-action="Register">Cadastro</a>
                </div>
            </div>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>
    <script src="~/js/site.js"></script>

</body>
</html>
